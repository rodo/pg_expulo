{
    "defaults":[
        { "name": "name", "generator": "FakeName" }
    ],
    "tables": [
        {"name": "boat",
         "schema": "public",
         "clean": "delete",
         "columns": [
             { "name": "id", "generator": "serial" },
             { "name": "name", "generator": "null" },
             { "name": "value",
                 "generator": "randomInt"
             },
             {
                 "name": "architect",
                 "generator": "foreign_key"
             }
         ]
        },
        {"name": "race",
         "schema": "public",
         "clean": "append",
         "filter": "profile='public'",
         "columns": [
             { "name": "id",   "generator": "serial" },
             { "name": "name", "generator": "null" }
         ]
        },
        {"name": "results",
         "schema": "public",
         "clean": "delete",
         "columns": [
             { "name": "id", "generator": "serial" },
             { "name": "race_id", "generator": "foreign_key" }
         ]
        },
        {"name": "architect",
         "schema": "public",
         "clean": "delete",
         "comment": "no conf, all is inherited",
         "columns": [
             { "name": "id", "generator": "serial" }
         ]
        },

        {"name": "skipper",
         "schema": "public",
         "clean": "delete",
         "columns": [
             {
                 "name": "id", "generator": "serial"
             },
             {
                 "name": "name",
                 "generator": "FakeName"
             },
             {
                 "name": "email",
                 "generator": "FakeEmail"
             },
             {
                 "name": "age",
                 "generator": "randomIntMinMax",
                 "min": 42,
                 "max": 42
             },
             {
                 "name": "updated_at",
