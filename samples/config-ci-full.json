                 "function": "now()"
             },
             {
                 "name": "town",
                 "generator": "md5"
             },
             {
                 "name": "country",
                 "generator": "randomString",
                 "preserve_null": true
             }
         ]
        },
        {"schema": "public",
         "name": "town",
         "clean": "delete",
         "deletion_filter": "area != 'South'",
         "columns": [
             {
                 "name": "name", "generator": "keep"
             }]
        }
    ]

}
